#include <stdlib.h>
#include <stdio.h>
#include <time.h>

int main()
{   int dimCol, dimRow;
     srand ( time(NULL) );
     printf("Input dimColumn = ");
     scanf("%i", &dimCol);
    printf("Input dimRow = ");
     scanf("%i", &dimRow);
     printf("\n");
    int array[dimRow][dimCol];
    printf("--------Generated array-------\n");
    for (int i =0; i<dimRow; i++){
        for(int j=0;j<dimCol; j++){
        //array[i][j]=100-rand()%200;
        printf("input array[%i][%i] = ", i, j);
        scanf("%i", &array[i][j]);
        }
    }
    
    for (int i =0; i<dimRow; i++){
        for(int j=0;j<dimCol; j++){
        printf("%i\t",array[i][j]);
        }
        printf("\n");
    }
   
}
